[colors]
draw_bold_text_with_bright_colors = true

[env]
TERM = "xterm-256color"

[font]
size = 9

[font.bold]
family = "JetBrainsMono Nerd Font Mono"
style = "Regular"

[font.bold_italic]
family = "JetBrainsMono Nerd Font Mono"
style = "Regular"

[font.bright]
black = "#6b7b88"
blue = "#a4cef6"
cyan = "#85cfcd"
green = "#8a9e7b"
magenta = "#e892d8"
red = "#d76b68"
white = "#c6d6f7"
yellow = "#c69e6d"

[font.footer_bar]
background = "#a89984"
foreground = "#f0f0f0"

[font.italic]
family = "JetBrainsMono Nerd Font Mono"
style = "Regular"

[font.normal]
black = "#0f101a"
blue = "#9abada"
cyan = "#97c9c7"
green = "#bdccb2"
magenta = "#dca5d2"
red = "#be5e67"
white = "#c9d2e4"
yellow = "#e98b4d"

[[keyboard.bindings]]
action = "Paste"
key = "V"
mods = "Command"

[[keyboard.bindings]]
action = "Copy"
key = "C"
mods = "Command"

[[keyboard.bindings]]
action = "Quit"
key = "Q"
mods = "Command"

[[keyboard.bindings]]
action = "Quit"
key = "W"
mods = "Command"

[[keyboard.bindings]]
action = "CreateNewWindow"
key = "T"
mods = "Command"

